{
    "extends": [
      "plugin:@next/next/recommended",
      "@rocketseat/eslint-config/next"
    ],
    "rules": {
      "prettier/prettier": ["error", {
        "printWidth": 120,
        "tabWidth": 2,
        "singleQuote": true,
        "trailingComma": "all",
        "arrowParens": "always",
        "semi": false,
        "endOfLine": "auto"
      }],
      "@typescript-eslint/no-explicit-any": "off",
        "@typescript-eslint/no-unused-vars": "warn"
      
    }
  }